# Function Tests
(def add (x y)                   
    (return (+ x y)))

(def factorial (n)                   #Complex function
    (let result 1)
    (let i 1)
    (while (<= i n)
        (*= result i)
        (+= i 1))
    (return result))

(print (factorial 5))                        # Function call
(print (add 3 4))                    # Function call with return value